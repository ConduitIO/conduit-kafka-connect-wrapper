{
  "@level": {
    "$resolver": "level",
    "field": "name"
  },
  "loggerName": {
    "$resolver": "logger",
    "field": "name"
  },
  "@message": {
    "$resolver": "message",
    "stringified": true
  },
  "thread": {
    "$resolver": "thread",
    "field": "name"
  },
  "exception": {
    "$resolver": "exception",
    "field": "stackTrace",
    "stackTrace": {
      "stringified": true
    }
  }
}
